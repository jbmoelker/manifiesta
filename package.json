{
  "name": "manifiesta",
  "version": "1.0.0",
  "description": "Manifiesta, manifest generator",
  "main": "index.js",
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-clean-css": "^3.4.0",
    "gulp-concat": "2.6.1",
    "gulp-if": "^2.0.2",
    "gulp-less": "^3.3.0",
    "gulp-nunjucks": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "2.0.0",
    "npm-run-all": "4.0.2",
    "nodemon": "^1.11.0",
    "nunjucks": "^3.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "chokidar-cli": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-nunjucks-render": "^2.2.1",
    "gulp-watch": "^4.3.11"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:assets": "node tasks/copy-assets.js",
    "build:js": "node tasks/build-js.js",
    "build:css": "node tasks/build-css.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:assets": "chokidar 'src/assets/**/*' --command 'npm run build:assets'",
    "watch:css": "chokidar 'src/**/*.less' --command 'npm run build:css'",
    "watch:js": "chokidar 'src/**/*.js' --command 'npm run build:js'",
    "start": "nodemon index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voorhoede/manifiesta.git"
  },
  "keywords": [
    "manifiesta"
  ],
  "author": "De Voorhoede",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/voorhoede/manifiesta/issues"
  },
  "homepage": "https://github.com/voorhoede/manifiesta#readme"
}
